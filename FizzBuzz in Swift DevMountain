for index in 1...100
{
     if (index % 3 == 0 && index % 5 == 0){
        print("DevMountain")
     }
        else if (index % 3 == 0){
        print("Dev")
    }
    else if (index % 5 == 0){
        print("Mountain")
    
    }
    print(index)
}


    


